<header id="header_main">
	<div class="header_main_content">
		<nav class="uk-navbar">
			 <sidebar-primary-toggle></sidebar-primary-toggle>

			<div class="uk-navbar-flip">
				<ul class="uk-navbar-nav user_actions">
					<li style="padding-top: 7px;" data-uk-dropdown="{mode:'click',pos:'bottom-right'}">
						<button class="md-fab md-fab-small md-ibebidas-laranja" style="font-size: 24px; height: 34px; width: 34px;">
                          	<span style="text-transform: capitalize;">{{usuarioLogado.nome| limitTo : 1 : 0}}</span>
                          </button>	
							
						<div class="uk-dropdown uk-dropdown-small">
							<h4 style="border-bottom: solid 1px #774386;" class="uk-text-center">{{usuarioLogado.nome}}</h4>
							<ul class="uk-nav js-uk-prevent">
								<li><a ng-click="logout()">Sair</a></li>
							</ul>
						</div></li>
				</ul>
			</div>
